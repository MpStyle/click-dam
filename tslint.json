{
  "rules": {
    "no-arg": true,
    "no-bitwise": true,
    "no-conditional-assignment": true,
    "no-consecutive-blank-lines": true,
    "indent": [
      true,
      "spaces",
      4
    ],
    "class-name": true,
    "ordered-imports": [
      true,
      {
        "import-sources-order": "lowercase-last",
        "named-imports-order": "lowercase-first"
      }
    ]
  }
}